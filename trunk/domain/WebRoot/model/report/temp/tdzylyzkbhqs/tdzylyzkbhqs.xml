<anychart>
  <settings>
    <animation enabled="True" />
  </settings>
  <charts>
    <chart type="CategorizedVertical">
      <chart_settings>
        <title>
          <text>土地利用资源变化趋势</text>
          <background enabled="false" />
        </title>
		<axes>
          <x_axis tickmarks_placement="Center" >
          	<title>
          		<text>年份</text>
          	</title>
		  </x_axis>
          <y_axis>
            <scale minimum="0" />
          	<title>
          		<text>用地量（亩）</text>
          	</title>			
          </y_axis>
        </axes>
      </chart_settings>
	  <data_plot_settings default_series_type="SplineArea">
        <area_series>
          <marker_settings enabled="True">
            <marker type="Circle" size="6" />
          </marker_settings>
          <tooltip_settings enabled="True">
            <format>{%YValue}</format>
          </tooltip_settings>
        </area_series>
      </data_plot_settings>
      <data>
        <series id="s1">
        </series>
        <series id="s2">
        </series>
        <series id="s3">
        </series>
      </data>
    </chart>
  </charts>
</anychart>