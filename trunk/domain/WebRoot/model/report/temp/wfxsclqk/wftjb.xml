<?xml version="1.0" encoding="UTF-8"?>
<anychart>
  <settings>
    <animation enabled="True" />
  </settings>
  <charts>
    <chart plot_type="CategorizedVertical">
      <chart_settings>
        <title>
          <text>土地和矿产违法趋势分析</text>
          <background enabled="false" />
		  	<font  bold="True" size="16" > 
            </font> 
        </title>
        <axes>
           <x_axis tickmarks_placement="Center">
            <title enabled="true">
              <text>行政区</text>
            </title>
          </x_axis>
          <y_axis tickmarks_placement="Center">
            <title enabled="true">
              <text>土地和矿产违法数量</text>
            </title>
          </y_axis>
          
        </axes>
      </chart_settings>
      <data_plot_settings default_series_type="Spline">
        <line_series>
          <tooltip_settings enabled="true">
            <format>
问题类型: {%SeriesName}
行政区: {%Name}
数量: {%YValue}{numDecimals: 0}件
</format>
          </tooltip_settings>
          <line_style>
            <line thickness="3" />
          </line_style>
        </line_series>
      </data_plot_settings>
      <data>
<series name="土地违法">
          <point name="广陵区" y="10" />
          <point name="邗江区	" y="12" />
          <point name="维扬区" y="22" />
          <point name="开发区" y="17" />
          <point name="江都区" y="23" />
          <point name="仪征区" y="15" />
 		  <point name="高邮区" y="18" /> 
		  <point name="宝应县" y="31" />
        </series>
        <series name="矿产违法">
          <point name="广陵区" y="0" />
          <point name="邗江区	" y="0" />
          <point name="维扬区" y="1" />
          <point name="开发区" y="0" />
          <point name="江都区" y="1" />
          <point name="仪征区" y="0" />
 		  <point name="高邮区" y="0" /> 
		  <point name="宝应县" y="1" />
        </series>
      </data>
    </chart>
  </charts>
</anychart>