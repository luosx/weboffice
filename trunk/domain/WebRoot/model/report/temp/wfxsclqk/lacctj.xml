<?xml version="1.0" encoding="UTF-8"?>
<anychart>
    <charts>
        <chart plot_type="Map">
            <chart_settings>
                <title>
                    <font size="18"/>
                    <text>
                        立案查处各县级单位数量统计
                    </text>
                </title>
                <legend enabled="True" position="Left" align="Far" ignore_auto_item="True" rows_padding="2" inside_dataplot="True">
                    <format>
                        {%Icon} {%RangeMin} - {%RangeMax}
                    </format>
                    <title>
                        <font size="12"/>
                        <text>
                            立案案件数量区间（件）
                        </text>
                    </title>
                    <title enabled="False"/>
                    <items>
                        <item source="Thresholds"/>
                    </items>
                </legend>
                <controls align_right_by="DataPlot">
                    <navigation_panel enabled="True"/>
                    <zoom_panel enabled="True"/>
                </controls>
            </chart_settings>
            
            <data_plot_settings>
                <map_series source="1.amap" id_column="NAME99">
                    <interactivity allow_select="false"/>
                    <border enabled="True" color="#777777" opacity="0.3"/>
					<zoom factor="1.4" y="0.56"/>
                    <projection type="Equirectangular"/>
                    <grid>
                        <background enabled="false"/>
                        <parallels enabled="True">
                            <major_line enabled="True"/>
                            <minor_line enabled="True"/>
                        </parallels>
                        <meridians enabled="true">
                        </meridians>
                    </grid>
                    <defined_map_region>
                        <label_settings enabled="True">
                            <format>
                                {%NAME99}
                            </format>
                            <font size="12"/>
                        </label_settings>
                        <tooltip_settings enabled="true">
                            <format>
                                {%NAME99}:{%YValue}{numDecimals:0}件
                            </format>
                            <font size="12"/>
                        </tooltip_settings>
                        <map_region_style>
                            <states>
                                <hover>
                                    <border enabled="true" color="#555555"/>
                                </hover>
                            </states>
                        </map_region_style>
                    </defined_map_region>
                </map_series>
            </data_plot_settings>
            
            <palettes>
                <palette name="custom" type="ColorRange" color_count="5">
                    <gradient>
                        <key color="green"/>
                        <key color="red"/>
                    </gradient>
                </palette>
                
            </palettes>
            
