<anychart>
	<settings>
		<animation enabled="true"/>
		<maps path="" path_type="RelativeToHTML"/>
	</settings>
<charts>
<chart plot_type="Pie">
      <data_plot_settings>
        <pie_series>
          <label_settings enabled="True">
            <font color="White">
              <effects>
                <drop_shadow enabled="True" />
              </effects>
            </font>
            <position anchor="Center" valign="Center" halign="Center" padding="0" />
            <format>{%YPercentOfSeries}{numDecimals:1}%</format>
          </label_settings>
        </pie_series>
      </data_plot_settings>
	<data>
        <series id="s1" name="tdzylyzk"  type="Line" palette="default">
			<label enabled="true">
				<animation  enabled="true" />
			</label>

        </series>
	
	</data>

	<chart_settings>
		<title enabled="true" padding="15">
			<text>土地资源利用状况</text>
		</title>
		<legend enabled="true" position="left" align="Spread" ignore_auto_item="true" padding="15">
				<format>{%Icon} {%Name} ({%YValue}亩)</format>
				<template></template>

				<title enabled="true">
					<text>土地资源利用明细</text>
				</title>
				<columns_separator enabled="false"/>
				<background>
					<inside_margin left="10" right="10"/>
				</background>
				<items>
					<item source="Points"/> 
				</items>
			</legend>
	</chart_settings>
</chart>
</charts>
</anychart>