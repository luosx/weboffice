<?xml version="1.0" encoding="UTF-8"?>
<anychart>
  <settings>
    <animation enabled="True" />
  </settings>
  <charts>
    <chart plot_type="CategorizedVertical">
      <chart_settings>
        <title>
          <text>线性图形</text>
          <background enabled="false" />
        </title>
      </chart_settings>
      <data_plot_settings default_series_type="Spline">
        <line_series>
          <marker_settings>
            <marker size="8" />
            <states>
              <hover>
                <marker size="12" />
              </hover>
            </states>
          </marker_settings>
          <tooltip_settings enabled="True" />
        </line_series>
      </data_plot_settings>
      <data>
        <series id="s1" name="Series 1">
        </series>
        <series  id="s2"  name="Series 2">
        </series>
      </data>
    </chart>
  </charts>
</anychart>