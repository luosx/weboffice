<?xml version="1.0" encoding="UTF-8"?>
<anychart>
  <dashboard>
    <view type="Dashboard">
      <title padding="2">
        <text>2010年度卫片执法检查图斑统计—（按卫片匹配分）</text>
      </title>
      <background>
        <inside_margin all="3" top="10" />
      </background>
      <vbox width="100%" height="100%">
        <margin all="0" />
        <hbox width="100%" height="50%">
          <margin all="0" />
          <view type="Chart" source="mainChart" width="100%" height="100%" />
        </hbox>
        <hbox width="100%" height="50%">
          <margin all="0" />
          <view name="q12View" type="Chart" source="all_num" width="50%" height="100%" />
          <view name="q34View" type="Chart" source="all_area" width="50%" height="100%" />
        </hbox>
      </vbox>
    </view>
  </dashboard>
  <templates>
    <template name="baseTemplate">
      <chart>
        <chart_settings>
          <chart_background>
            <border type="Solid" color="#CCCCCC" thickness="1" />
            <corners type="Square" />
            <effects enabled="false" />
            <inside_margin all="10" top="5" />
          </chart_background>
          <title enabled="true" position="Bottom" />
        </chart_settings>
        <data_plot_settings>
          <pie_series>
            <label_settings enabled="True" mode="Outside" multi_line_align="Center">
              <position halign="Center" valign="Center" padding="8" />
              <format>{%Name}, {%Value}</format>
              <font bold="False" />
            </label_settings>
            <connector enabled="True" opacity="0.3" />
            <tooltip_settings enabled="True">
              <format>{%Name} - {%Value}</format>
            </tooltip_settings>
          </pie_series>
        </data_plot_settings>
      </chart>
    </template>
  </templates>
  <charts>
    <chart name="mainChart" plot_type="CategorizedVertical">
      <chart_settings>
        <title enabled="False" align="Left" align_by="DataPlot" padding="0">
          <text>Select a column to Drill-Down</text>
        </title>
        <chart_background>
          <border type="Solid" color="#CCCCCC" thickness="1" />
          <corners type="Square" />
          <effects enabled="false" />
          <inside_margin all="10" top="5" />
        </chart_background>
        <axes>
          <y_axis position="Left">
            <title enabled="False" />
            <scale type="Logarithmic" logarithmic_base="20" minimum="1" />
          </y_axis>
          <x_axis>
            <title enabled="False" />
          </x_axis>
        </axes>
      </chart_settings>
      <data_plot_settings>
        <bar_series group_padding="0.3">
          <marker_settings enabled="true" color="Gold">
            <marker type="None" size="10" />
            <states>
              <hover>
                <marker type="Circle" />
                <border thickness="2" />
              </hover>
              <pushed>
                <marker type="Circle" size="6" />
                <border thickness="2" />
              </pushed>
              <selected_normal>
                <marker type="Star5" size="16" />
                <border thickness="1" />
              </selected_normal>
              <selected_hover>
                <marker type="Star5" size="12" />
                <border thickness="1" />
              </selected_hover>
            </states>
          </marker_settings>
          <tooltip_settings enabled="true">
            <position anchor="CenterTop" padding="10" />
            <background>
              <border color="DarkColor(Gold)" thickness="1" />
            </background>
          </tooltip_settings>
        </bar_series>
      </data_plot_settings>
      <data>
        <series>
          <point name="广陵区" y="224">
            <actions>
               <action type="UpdateView" view="q12View" source="glq_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="glq_area" source_mode="InternalData" />
            </actions>
          </point>
		  <point name="维扬区" y="190">
            <actions>
               <action type="UpdateView" view="q12View" source="wyq_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="wyq_area" source_mode="InternalData" />
            </actions>
          </point>
		 <point name="邗江区" y="201">
            <actions>
               <action type="UpdateView" view="q12View" source="hjq_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="hjq_area" source_mode="InternalData" />
            </actions>
          </point>  
          <point name="开发区" y="137">
            <actions>
              <action type="UpdateView" view="q12View" source="kfq_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="kfq_area" source_mode="InternalData" />
            </actions>
          </point>
          <point name="仪征市" y="99">
            <actions>
               <action type="UpdateView" view="q12View" source="yzs_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="yzs_area" source_mode="InternalData" />
            </actions>
          </point>
          <point name="高邮市" y="77">
            <actions>
               <action type="UpdateView" view="q12View" source="gys_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="gys_area" source_mode="InternalData" />
            </actions>
          </point>
          <point name="江都市" y="96">
            <actions>
               <action type="UpdateView" view="q12View" source="jds_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="jds_area" source_mode="InternalData" />
            </actions>
          </point>
          <point name="宝应县" y="80">
            <actions>
               <action type="UpdateView" view="q12View" source="byx_num" source_mode="InternalData" />
              <action type="UpdateView" view="q34View" source="byx_area" source_mode="InternalData" />
            </actions>
          </point>
        </series>
      </data>
    </chart>
	
