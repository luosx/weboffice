<?xml version="1.0" encoding="UTF-8"?>
<anychart>
<dashboard>
		<view type="Dashboard">
			<title padding="0">
				<text>山东省青岛市卫片图斑统计柱状图</text>
			</title>
			<background>
				<inside_margin all="3" top="10" />
			</background>

			<vbox width="100%" height="100%">
				<view name="v1" type="Chart" source="mainData" width="100%"
					height="100%">
					<margin top="0" />
				</view>
			</vbox>
		</view>
	</dashboard>
	<settings>
		<animation enabled="True" />
	</settings>
	<charts>
		<chart plot_type="CategorizedVertical" name="mainData">
			<data_plot_settings default_series_type="Bar">
				<bar_series group_padding="0.2">
					<tooltip_settings enabled="true" />
				</bar_series>
			</data_plot_settings>
			<chart_settings>
				<title enabled="true">
					<text></text>
				</title>
				 <axes>
           <x_axis tickmarks_placement="Center">
            <title enabled="true">
              <text>地区</text>
            </title>
          </x_axis>
          <y_axis tickmarks_placement="Center" >
            <title enabled="true">
              <text>图斑数量（单位：个）</text>
            </title>
          </y_axis>
        </axes>
			</chart_settings>
			<data>
				<series id="s1">

				</series>
			</data>
		</chart>
	</charts>
</anychart>