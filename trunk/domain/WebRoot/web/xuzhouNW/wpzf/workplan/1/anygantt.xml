<?xml version="1.0" encoding="UTF-8"?>
<anygantt>
    <styles>
        <defaults>
            <summary>
                <task_style>
                    <tooltip enabled="true">
                        <font render_as_html="true"/>
                        <text>
                            <![CDATA[<font face="Verdana" size="10"><p><font color="#770000" face="Verdana" size="10"><b>{%Name}</b></font></p>
开始时间: <p><font color="#007700">{%ActualStart}{dateTimeFormat: %yyyy/%MMM/%d}</font></p>
结束时间:  <p><font color="#007700">{%ActualEnd}{dateTimeFormat: %yyyy/%MMM/%d}</font></p>
所需时间: <p><font color="#0055AA"><b>{%Duration}</b></font></p>
完成度: <p><font color="#494949"><b>{%Complete}%</b></font></p></font>
]]>
                        </text>
                    </tooltip>
                    <row>
                        <fill enabled="false"/>
                    </row>
                    <row_datagrid>
                        <cell>
                            <font bold="true" size="12"/>
                            <states>
                                <hover>
                                    <font underline="true"/>
                                </hover>
                                <pushed>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </pushed>
                                <selected_normal>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </selected_normal>
                                <selected_hover>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </selected_hover>
                            </states>
                        </cell>
                    </row_datagrid>
                    <actual>
                        <bar_style>
                            <labels>
                                <label anchor="Right" valign="Center" halign="Far">
                                    <text>
                                        {%Complete}%
                                    </text>
                                </label>
                                <!--
                                <label anchor="Left" valign="Center" halign="Near">
                                <text>{%Name}</text>
                                </label>
                                -->
                            </labels>
                            <middle shape="HalfTop">
                                <border type="Solid" color="rgb(252,2,4)"/>
                                <fill type="Solid" color="rgb(252,190,4)"/>
                            </middle>
                            <start>
                                <marker type="Arrow">
                                    <border type="Solid" color="rgb(252,2,4)"/>
                                    <fill type="Solid" color="#FF4500"/>
                                </marker>
                            </start>
                            <end>
                                <marker type="Arrow">
                                    <border type="Solid" color="rgb(252,2,4)"/>
                                    <fill type="Solid" color="#FF4500"/>
                                </marker>
                            </end>
                            <states>
                                <hover>
                                    <middle shape="HalfTop">
                                        <border type="Solid" color="#000000"/>
                                        <fill type="Solid" color="#777777"/>
                                    </middle>
                                    <start>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#777777"/>
                                        </marker>
                                    </start>
                                    <end>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#777777"/>
                                        </marker>
                                    </end>
                                </hover>
                                <selected_hover>
                                    <middle shape="HalfTop">
                                        <border type="Solid" color="#000000"/>
                                        <fill type="Solid" color="#E77373"/>
                                    </middle>
                                    <start>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#E77373"/>
                                        </marker>
                                    </start>
                                    <end>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#E77373"/>
                                        </marker>
                                    </end>
                                </selected_hover>
                                <selected_normal>
                                    <middle shape="HalfTop">
                                        <border type="Solid" color="#000000"/>
                                        <fill type="Solid" color="#E77373"/>
                                    </middle>
                                    <start>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#E77373"/>
                                        </marker>
                                    </start>
                                    <end>
                                        <marker type="Arrow">
                                            <border type="Solid" color="#000000"/>
                                            <fill type="Solid" color="#E77373"/>
                                        </marker>
                                    </end>
                                </selected_normal>
                            </states>
                        </bar_style>
                    </actual>
                    <progress>
                        <bar_style>
                            <middle shape="ThinBottom">
                                <fill type="Solid" color="rgb(68,178,252)"/>
                                <border enabled="true" color="#665300"/>
                            </middle>
                        </bar_style>
                    </progress>
                </task_style>
            </summary>
            <task>
                <task_style>
                    <tooltip enabled="true">
                        <font render_as_html="true"/>
                        <text>
                            <![CDATA[<font face="Verdana" size="10"><p><font color="#770000" face="Verdana" size="10"><b>{%Name}</b></font></p>
开始时间 : <p><font color="#007700">{%ActualStart}{dateTimeFormat: %yyyy/%MMM/%d}</font></p>
结束时间:  <p><font color="#007700">{%ActualEnd}{dateTimeFormat: %yyyy/%MMM/%d}</font></p>
所需时间: <p><font color="#0055AA"><b>{%Duration}</b></font></p>
完成度: <p><font color="#494949"><b>{%Complete}%</b></font></p></font>
]]>
                        </text>
                    </tooltip>
                    
                    <row>
                        <fill enabled="false"/>
                    </row>
                    <row_datagrid>
                        <cell>
                            <states>
                                <hover>
                                    <font underline="true"/>
                                </hover>
                                <pushed>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </pushed>
                                <selected_normal>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </selected_normal>
                                <selected_hover>
                                    <fill type="Solid" color="#E77373" opacity="0.5"/>
                                </selected_hover>
                            </states>
                        </cell>
                    </row_datagrid>
                    <actual>
                        <bar_style>
                            <labels>
                                <label anchor="Right" valign="Center" halign="Far">
                                    <text>
                                        {%Complete}%
                                    </text>
                                </label>
                                <!--
                                <label anchor="Left" valign="Center" halign="Near">
                                <text>{%Name}</text>
                                </label>
                                -->
                            </labels>
                            <middle shape="Full">
                                <fill type="Solid" color="#D1EEEE"/>
                                <border thickness="2" caps="Round" type="Solid" color="#4876FF"/>
                            </middle>
                            <states>
                                <hover>
                                    <middle shape="Full">
                                        <fill type="Solid" color="#E7EEFC"/>
                                        <border type="Solid" color="#7A88A6"/>
                                    </middle>
                                </hover>
                                <selected_hover>
                                    <middle shape="Full">
                                        <fill type="Solid" color="#FCBDCD"/>
                                        <border type="Solid" color="#A67A83"/>
                                    </middle>
                                </selected_hover>
                                <selected_normal>
                                    <middle shape="Full">
                                        <fill type="Solid" color="#FCBDCD"/>
                                        <border type="Solid" color="#A67A83"/>
                                    </middle>
                                </selected_normal>
                            </states>
                        </bar_style>
                    </actual>
                    <progress>
                        <bar_style>
                            <middle shape="HalfCenter">
                                <fill type="Solid" color="#836FFF"/>
                                <border type="Solid" caps="Round" color="#4463A6"/>
                            </middle>
                            <states>
                                <hover>
                                    <middle shape="ThinCenter">
                                        <fill type="Solid" color="#9997F9"/>
                                        <border type="Solid" color="#4463A6"/>
                                    </middle>
                                </hover>
                                <selected_normal>
                                    <middle shape="ThinCenter">
                                        <fill type="Solid" color="#F97968"/>
                                        <border type="Solid" color="#A64459"/>
                                    </middle>
                                </selected_normal>
                                <selected_hover>
                                    <middle shape="ThinCenter">
                                        <fill type="Solid" color="#F97968"/>
                                        <border type="Solid" color="#A64459"/>
                                    </middle>
                                </selected_hover>
                            </states>
                        </bar_style>
                    </progress>
                </task_style>
            </task>
            <milestone>
                <task_style>
                    <tooltip enabled="true">
                        <font render_as_html="true"/>
                        <text>
                            <![CDATA[<p><font color="#770000" face="Verdana" size="10"><b>{%Name}</b></font></p>
<p><font color="#007700" face="Verdana" size="10"><b>{%ActualStart}</b></font></p>
]]>
                        </text>
                    </tooltip>
                    <actual>
                        <bar_style>
                            <labels>
                                <label anchor="Left" valign="Center" halign="Near">
                                    <text>
                                        {%Name}
                                    </text>
                                </label>
                            </labels>
                            <start>
                                <marker type="Rhomb">
                                    <border type="Solid" color="Green"/>
                                    <fill type="Solid" color="#54D554"/>
                                </marker>
                            </start>
                            <states>
                                <hover>
                                    <start>
                                        <marker type="Rhomb">
                                            <border type="Solid" color="Green"/>
                                            <fill type="Solid" color="#8BF78B"/>
                                        </marker>
                                    </start>
                                </hover>
                                <selected_hover>
                                    <start>
                                        <marker type="Rhomb">
                                            <fill type="Solid" color="#F97968"/>
                                            <border type="Solid" color="#A64459"/>
                                        </marker>
                                    </start>
                                </selected_hover>
                                <selected_normal>
                                    <start>
                                        <marker type="Rhomb">
                                            <fill type="Solid" color="#F97968"/>
                                            <border type="Solid" color="#A64459"/>
                                        </marker>
                                    </start>
                                </selected_normal>
                            </states>
                        </bar_style>
                    </actual>
                </task_style>
            </milestone>
            <connector>
                <connector_style>
                    <line type="Solid" thickness="2" color="blue"/>
                </connector_style>
            </connector>
        </defaults>
    </styles>
    <settings>
        <title>
            <text>
                test
            </text>
        </title>
        <locale>
            <date_time_format>
                <week_days>
                    <names>
                        星期日,星期一,星期二,星期三,星期四,星期五,星期六
                    </names>
                    <short_names>
                        星期日,星期一,星期二,星期三,星期四,星期五,星期六
                    </short_names>
                </week_days>
                <months>
                    <names>
                        一月,二月,三月,四月,五月,六月,七月,八月,九月,十月,十一月,十二月
                    </names>
                    <short_names>
                        一月,二月,三月,四月,五月,六月,七月,八月,九月,十月,十一月,十二月
                    </short_names>
                </months>
                <format>
                    <date>
                        YYYY/MM/dd
                    </date>
                </format>
            </date_time_format>
        </locale>
        
        <navigation enabled="True" position="Top" size="45">
            <buttons align="Far"/>
            <text>
                卫片执法工作计划
            </text>
            <font face="微软雅黑" size="18" bold="true" color="White"/>
            <background>
                <fill type="Gradient">
                    <gradient>
                               
                                <key color="rgb(35,110,177)" position="0"/>
                                <key color="rgb(27,106,175)" position="0.2"/>
                                <key color="rgb(21,107,184)" position="0.5"/>
								<key color="rgb(0,95,179)" position="0.7"/>
                                <key color="rgb(7,98,179)" position="0.9"/>
                                <key color="rgb(26,104,170)" position="1"/>
                    </gradient>
                </fill>
                
                <border type="Solid" color="#494949"/>
            </background>
        </navigation>
        <background enabled="true"/>
    </settings>
    <datagrid enabled="true" show_tooltip="False">
        <columns>
            <column width="30">
                <header>
                    <text>
                        
                    </text>
                </header>
                <format>
                    {%RowNum}
                </format>
            </column>
            <column cell_align="LeftLevelPadding" width="200">
                <header>
                    <text>
                        任 务 名 称
                    </text>
                    <cell>
                        <font size="12" bold="true"/>
                        <fill type="Gradient">
                            <gradient>
                                <key color="rgb(252,254,252)" position="0"/>
                                <key color="rgb(252,250,252)" position="0.3"/>
                                <key color="rgb(244,246,245)" position="0.5"/>
                                <key color="rgb(244,242,236)" position="0.7"/>
                                <key color="rgb(236,234,236)" position="1"/>
                            </gradient>
                        </fill>
                        <states>
                            <hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </hover>
                            <pushed>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#BCD2EE"/>
                                    </gradient>
                                </fill>
                            </pushed>
                            <selected_hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </selected_hover>
                        </states>
                    </cell>
                </header>
                <format>
                    {%Name}
                </format>
            </column>
            <column>
                <header>
                    <text>
                        开始时间
                    </text>
                    <cell>
                        <font size="12" bold="true"/>
                        <fill type="Gradient">
                            <gradient>
                                <key color="rgb(252,254,252)" position="0"/>
                                <key color="rgb(252,250,252)" position="0.3"/>
                                <key color="rgb(244,246,245)" position="0.5"/>
                                <key color="rgb(244,242,236)" position="0.7"/>
                                <key color="rgb(236,234,236)" position="1"/>
                            </gradient>
                        </fill>
                        <states>
                            <hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </hover>
                            <pushed>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#BCD2EE"/>
                                    </gradient>
                                </fill>
                            </pushed>
                            <selected_hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </selected_hover>
                        </states>
                    </cell>
                </header>
                <format>
                    {%ActualStart}
                </format>
            </column>
            <column>
                <header>
                    <text>
                        结束时间
                    </text>
                    <cell>
                        <font size="12" bold="true"/>
                        <fill type="Gradient">
                            <gradient>
                                <key color="rgb(252,254,252)" position="0"/>
                                <key color="rgb(252,250,252)" position="0.3"/>
                                <key color="rgb(244,246,245)" position="0.5"/>
                                <key color="rgb(244,242,236)" position="0.7"/>
                                <key color="rgb(236,234,236)" position="1"/>
                            </gradient>
                        </fill>
                        <states>
                            <hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </hover>
                            <pushed>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#BCD2EE"/>
                                    </gradient>
                                </fill>
                            </pushed>
                            <selected_hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </selected_hover>
                        </states>
                    </cell>
                </header>
                <format>
                    {%ActualEnd}
                </format>
            </column>
            <column>
                <header>
                    <text>
                        完成进度
                    </text>
                    <cell>
                        <font size="12" bold="true"/>
                        <fill type="Gradient">
                            <gradient>
                                <key color="rgb(252,254,252)" position="0"/>
                                <key color="rgb(252,250,252)" position="0.3"/>
                                <key color="rgb(244,246,245)" position="0.5"/>
                                <key color="rgb(244,242,236)" position="0.7"/>
                                <key color="rgb(236,234,236)" position="1"/>
                            </gradient>
                        </fill>
                        <states>
                            <hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </hover>
                            <pushed>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#BCD2EE"/>
                                    </gradient>
                                </fill>
                            </pushed>
                            <selected_hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </selected_hover>
                        </states>
                    </cell>
                </header>
                <format>
                    {%Complete}%
                </format>
            </column>
            <column>
                <header>
                    <text>
                        所需时间
                    </text>
                    <cell>
                        <font size="12" bold="true"/>
                        <fill type="Gradient">
                            <gradient>
                                <key color="rgb(252,254,252)" position="0"/>
                                <key color="rgb(252,250,252)" position="0.3"/>
                                <key color="rgb(244,246,245)" position="0.5"/>
                                <key color="rgb(244,242,236)" position="0.7"/>
                                <key color="rgb(236,234,236)" position="1"/>
                            </gradient>
                        </fill>
                        <states>
                            <hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </hover>
                            <pushed>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#BCD2EE"/>
                                    </gradient>
                                </fill>
                            </pushed>
                            <selected_hover>
                                <fill type="Gradient">
                                    <gradient>
                                        <key color="#B2DFEE"/>
                                    </gradient>
                                </fill>
                            </selected_hover>
                        </states>
                    </cell>
                </header>
                <format>
                    {%Duration}
                </format>
            </column>
        </columns>
    </datagrid>
    <timeline>
        <plot line_height="24" item_height="16">
            <non_working_days show="true"/>
            <non_working_hours show="true"/>
			<current_time>
				<line thickness="3" color="#FF0000">
					
				</line>
		    </current_time>
        </plot>
        <scale show_start="2000.1.15 0.0" show_end="2013.2.28 0.0" lines="3" height="60">
            <zoom allow_animation="true"/>
            <bottom_line color="#696969"/>
        </scale>
    </timeline>
    
    <project_chart>
