<?xml version="1.0" encoding="UTF-8"?>
<anychart>
  <settings>
    <animation enabled="True" />
  </settings>
  <charts>
    <chart plot_type="CategorizedVertical">
      <chart_settings>
        <title enabled="true">
          <font size="30" family="黑体" />
          <text>二手房售价分析</text>
        </title>
        <axes>
          <y_axis position="Left">
            <scale minimum="0" maximum="50000" major_interval="5000" show_last_label="false" />
            <title enabled="true">
              <text>售价(元/平方米)</text>
            </title>
            <axis_markers>
              <ranges>
                <range minimum="0" maximum="5000">
                  <fill color="#C0C0C0" opacity="0.7" />
                </range>
              </ranges>
            </axis_markers>
          </y_axis>
          <x_axis>
            <title enabled="true">
              <text>时序(月)</text>
            </title>
          </x_axis>
        </axes>
        <legend enabled="true" position="Bottom" align="Spread" ignore_auto_item="true" padding="15">
          <format>{%Icon} {%Name}</format>
          <font size="30" />
          <template />
          <title enabled="false" />
          <columns_separator enabled="false" />
          <background>
            <inside_margin left="10" right="10" />
          </background>
          <items>
            <item source="series" />
          </items>
        </legend>
      </chart_settings>
      <data_plot_settings>
        <line_series>
          <line_style>
            <line thickness="3" />
          </line_style>
          <tooltip_settings enabled="true" />
        </line_series>
        <bar_series>
          <tooltip_settings enabled="True" />
        </bar_series>
      </data_plot_settings>
      <data>
        <series id="0" name="售价" type="Line" color="yellow">
          <point name="2014年3月" y="0" />
          <point name="2014年4月" y="0" />
          <point name="2014年5月" y="0" />
          <point name="2014年6月" y="0" />
          <point name="2014年7月" y="0" />
          <point name="2014年8月" y="0" />
          <point name="2014年9月" y="0" />
          <point name="2014年10月" y="0" />
          <point name="2014年11月" y="0" />
          <point name="2014年12月" y="0" />
          <point name="2014年1月" y="0" />
          <point name="2014年2月" y="5000" />
        </series>
      </data>
    </chart>
  </charts>
</anychart>

