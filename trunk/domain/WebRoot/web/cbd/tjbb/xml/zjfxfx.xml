<?xml version="1.0" encoding="UTF-8"?>
<anychart>
    <settings>
        <animation enabled="True"/>
    </settings>
    <charts>
        <chart plot_type="CategorizedVertical">
            
            <chart_settings>
                <title enabled="true">
                    <font size="30" family="黑体"/>
                    <text>
                        土地储备年度实施计划——资金风险分析
                    </text>
                </title>
                <axes>
                    <y_axis position="Left">
                        <scale minimum="0" maximum="90" major_interval="10" show_last_label="false"/>
                        <title enabled="true"><text>资金(亿元)</text></title>
                        <axis_markers>
                            <ranges>
                                <range minimum="0" maximum="90">
                                    <fill color="#C0C0C0" opacity="0.7"/>                
                                </range>                        
                            </ranges>
                        </axis_markers>
                    </y_axis>
                    <x_axis>
                        <title enabled="true"><text>时序(年)</text></title>
                    </x_axis>
                </axes>
                <legend enabled="true" position="Bottom" align="Spread" ignore_auto_item="true" padding="15">
                    <format>
                        {%Icon} {%Name}
                    </format>
                    <font size="30"/>
                    <template>
                    </template>
                    
                    <title enabled="false"/>

                    <columns_separator enabled="false"/>
                    <background>
                        <inside_margin left="10" right="10"/>
                    </background>
                    <items>
                        <item source="series"/>
                    </items>
                </legend>
            </chart_settings>
            <data_plot_settings>
                <line_series>
                    <line_style>
                        <line thickness="3"/>
                    </line_style>
                    <tooltip_settings enabled="true">
                    
                    </tooltip_settings>
                </line_series>
                <bar_series>
                    <tooltip_settings enabled="True">
                        
                    </tooltip_settings>
                </bar_series>
            </data_plot_settings>
            <data>
                <series name="非债务资金流入规模(流)" type="Line" color="yellow">
                    <point name="2013年" y="40"/>
                    <point name="2014年" y="47"/>
                    <point name="2015年" y="28"/>
                    <point name="2016年" y="0"/>
                    <point name="2017年" y="5"/>
                    <point name="2018年" y="0"/>
                    <point name="2019年" y="3"/>
                    <point name="2020年" y="0"/>
                    <point name="2021年" y="2"/>
                </series>
                
                <series name="债务性资金还款规模(流)" type="Line" color="red">
                    <point name="2013年" y="0"/>
                    <point name="2014年" y="0"/>
                    <point name="2015年" y="50"/>
                    <point name="2016年" y="0"/>
                    <point name="2017年" y="45"/>
                    <point name="2018年" y="21"/>
                    <point name="2019年" y="61"/>
                    <point name="2020年" y="29"/>
                    <point name="2021年" y="84"/>
                </series>
				
                <series name="年度投资规模(流)" type="Line" color="blue">
                    <point name="2013年" y="44"/>
                    <point name="2014年" y="22"/>
                    <point name="2015年" y="21"/>
                    <point name="2016年" y="22"/>
                    <point name="2017年" y="21"/>
                    <point name="2018年" y="8"/>
                    <point name="2019年" y="23"/>
                    <point name="2020年" y="0"/>
                    <point name="2021年" y="0"/>
                </series>				
				
            </data>
            
        </chart>
    </charts>
</anychart>

