<?xml version="1.0" encoding="UTF-8"?>
<anychart>
  <settings>
    <animation enabled="True" />
  </settings>
  <charts>
    <chart plot_type="CategorizedVertical">
      <chart_settings>
        <title enabled="true">
          <font size="30" family="黑体" />
          <text>土地储备年度实施计划—投资比例分析</text>
        </title>
        <axes>
          <y_axis position="Left">
            <scale minimum="0" maximum="90" major_interval="10" show_last_label="false" />
            <title enabled="true">
              <text>资金(亿元)</text>
            </title>
            <axis_markers>
              <ranges>
                <range minimum="0" maximum="90">
                  <fill color="#C0C0C0" opacity="0.7" />
                </range>
              </ranges>
            </axis_markers>
          </y_axis>
          <x_axis>
            <title enabled="true">
              <text>时序(年)</text>
            </title>
          </x_axis>
        </axes>
        <legend enabled="true" position="Bottom" align="Spread" ignore_auto_item="true" padding="15">
          <format>{%Icon} {%Name}</format>
          <font size="30" />
          <template />
          <title enabled="false" />
          <columns_separator enabled="false" />
          <background>
            <inside_margin left="10" right="10" />
          </background>
          <items>
            <item source="series" />
          </items>
        </legend>
      </chart_settings>
      <data_plot_settings>
        <line_series>
          <line_style>
            <line thickness="3" />
          </line_style>
          <tooltip_settings enabled="true" />
        </line_series>
        <bar_series>
          <tooltip_settings enabled="True" />
        </bar_series>
      </data_plot_settings>
      <data>
        <series id="0" name="自有资金使用规模" type="Bar" color="yellow">
          <point name="2013" y="-20.98" />
          <point name="2014" y="4.08" />
          <point name="2015" y="43.52" />
          <point name="2016" y="-22" />
          <point name="2017" y="-37" />
          <point name="2018" y="22" />
          <point name="2019" y="37" />
          <point name="2020" y="-34" />
        </series>
        <series id="1" name="债务资金使用规模" type="Bar" color="red">
          <point name="2013" y="37" />
          <point name="2014" y="0" />
          <point name="2015" y="-37" />
          <point name="2016" y="22" />
          <point name="2017" y="37" />
          <point name="2018" y="-22" />
          <point name="2019" y="-37" />
          <point name="2020" y="34" />
        </series>
      </data>
    </chart>
  </charts>
</anychart>

