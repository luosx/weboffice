<?xml version="1.0" encoding="UTF-8"?>
<anychart>
    <settings>
        <animation enabled="True"/>
    </settings>
    <charts>
        <chart plot_type="CategorizedVertical">
            
            <chart_settings>
                <title enabled="true">
                    <font size="40" family="黑体"/>
                    <text>
                        信访举报立案率同比统计
                    </text>
                </title>
                <axes>
                    <y_axis position="Left">
                        <scale minimum="0" maximum="100" major_interval="10" show_last_label="false"/>
                        <title enabled="false"/>
                        <axis_markers>
                            <ranges>
                                <range minimum="0" maximum="25">
                                    <fill color="green" opacity="0.7"/>
                                    <minimum_line color="#AEA4CC" opacity="1"/>
                                    <maximum_line color="#AEA4CC" opacity="1"/>
                                    <label enabled="True" position="Center">
                                        <format>
                                            低立案率
                                        </format>
                                        <font bold="True" color="#393939"/>
                                    </label>
                                </range>
                                <range minimum="25" maximum="50">
                                    <fill color="yellow" opacity="0.7"/>
                                    <minimum_line color="#96BC99" opacity="1"/>
                                    <maximum_line color="#96BC99" opacity="1"/>
                                    <label enabled="True" position="Far" multi_line_align="Center">
                                        <font bold="true" color="#393939"/>
                                        <format>
                                            普通立案率
                                        </format>
                                    </label>
                                </range>
                                <range minimum="50" maximum="100">
                                    <fill color="#E066FF" opacity="0.7"/>
                                    <minimum_line color="#CCCCA4" opacity="1"/>
                                    <maximum_line color="#CCCCA4" opacity="1"/>
                                    <label enabled="True" position="Center" multi_line_align="Center">
                                        <font bold="true" color="#393939"/>
                                        <format>
                                            高立案率
                                        </format>
                                    </label>
                                </range>
                                
                            </ranges>
                        </axis_markers>
                    </y_axis>
                    <x_axis>
                        <title enabled="false"/>
                    </x_axis>
                </axes>
                <legend enabled="true" position="Bottom" align="Spread" ignore_auto_item="true" padding="15">
                    <format>
                        {%Icon} {%Name}
                    </format>
                    <font size="30"/>
                    <template>
                    </template>
                    
                    <title enabled="true">
                        <font size="20"/>
                        <text size="35">
                            案件类型
                        </text>
                    </title>
                    <columns_separator enabled="false"/>
                    <background>
                        <inside_margin left="10" right="10"/>
                    </background>
                    <items>
                        <item source="series"/>
                    </items>
                </legend>
            </chart_settings>
            <data_plot_settings>
                <line_series>
                    <line_style>
                        <line thickness="3"/>
                    </line_style>
                    <tooltip_settings enabled="true">
                        <format>
                            当前月份立案率：{%Value}{numDecimals:0}%
                        </format>
                    </tooltip_settings>
                </line_series>
                <bar_series>
                    <tooltip_settings enabled="True">
                        <format>
                            当前月份立案率：{%Value}{numDecimals:0}%
                        </format>
                    </tooltip_settings>
                </bar_series>
            </data_plot_settings>
            <data>
                <series name="2011年群众举报立案率" type="Line" color="#F1683C">
                    <point name="1月份" y="33.69"/>
                    <point name="2月份" y="35"/>
                    <point name="3月份" y="30"/>
                    <point name="4月份" y="35"/>
                    <point name="5月份" y="50"/>
                    <point name="6月份" y="58"/>
                    <point name="7月份" y="50"/>
                    <point name="8月份" y="45"/>
                    <point name="9月份" y="55"/>
                    <point name="10月份" y="45"/>
					<point name="11月份" y="30"/>
					<point name="12月份" y="20"/>
                </series>
                
                <series name="2012年群众举报立案率" type="Line" color="#1D8BD1">
                    <point name="1月份" y="20"/>
                    <point name="2月份" y="30"/>
                    <point name="3月份" y="31"/>
                    <point name="4月份" y="40"/>
                    <point name="5月份" y="45"/>
                    <point name="6月份" y="50"/>
                    <point name="7月份" y="48"/>
                    <point name="8月份" y="50"/>
                    <point name="9月份" y="45"/>
                    <point name="10月份" y="40"/>
					<point name="11月份" y="30"/>
					<point name="12月份" y="21"/>
                </series>
            </data>
            
        </chart>
    </charts>
</anychart>

