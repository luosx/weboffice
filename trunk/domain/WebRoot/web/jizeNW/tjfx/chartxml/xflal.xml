<?xml version="1.0" encoding="UTF-8"?>
<anychart>
    <settings>
        <animation enabled="True"/>
    </settings>
    <charts>
        <chart plot_type="CategorizedVertical">
            
            <chart_settings>
                <title enabled="true">
                    <font size="40" family="黑体"/>
                    <text>
                        信访举报立案率统计
                    </text>
                </title>
                <axes>
                    <y_axis position="Left">
                        <scale minimum="0" maximum="100" major_interval="10" show_last_label="false"/>
                        <title enabled="false"/>

                    </y_axis>
                    <x_axis>
                        <title enabled="false"/>
                    </x_axis>
                </axes>
            </chart_settings>
            <data_plot_settings>
                <line_series>
                    <line_style>
                        <line thickness="3"/>
                    </line_style>
                    <tooltip_settings enabled="true">
                        <format>
                            当前月份立案率：{%Value}{numDecimals:0}%
                        </format>
                    </tooltip_settings>
                </line_series>
                <bar_series>
                    <tooltip_settings enabled="True">
                        <format>
                            当前月份立案率：{%Value}{numDecimals:0}%
                        </format>
                    </tooltip_settings>
                </bar_series>
            </data_plot_settings>
            <data>
                <series name="Series 1" type="Line" color="#F1683C">
                    <point name="1月份" y="33.69"/>
                    <point name="2月份" y="35"/>
                    <point name="3月份" y="40"/>
                    <point name="4月份" y="45"/>
                    <point name="5月份" y="50"/>
                    <point name="6月份" y="38"/>
                    <point name="7月份" y="30"/>
                    <point name="8月份" y="35"/>
                    <point name="9月份" y="40"/>
                    <point name="10月份" y="45"/>
                </series>
                
                <series name="Series 2" type="Bar" color="#1D8BD1">
                    <point name="1月份" y="33.69"/>
                    <point name="2月份" y="35"/>
                    <point name="3月份" y="40"/>
                    <point name="4月份" y="45"/>
                    <point name="5月份" y="50"/>
                    <point name="6月份" y="38"/>
                    <point name="7月份" y="30"/>
                    <point name="8月份" y="35"/>
                    <point name="9月份" y="40"/>
                    <point name="10月份" y="45"/>
                </series>
            </data>
            
        </chart>
    </charts>
</anychart>

