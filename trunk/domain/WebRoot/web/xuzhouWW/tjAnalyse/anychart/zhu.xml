<?xml version="1.0" encoding="UTF-8"?>
<anychart>
    <settings>
        <animation enabled="True"/>
    </settings>
    <charts>
        <chart plot_type="CategorizedVertical">
            <data_plot_settings default_series_type="Bar">
                <bar_series group_padding="0.2">
                    <tooltip_settings enabled="True">
                        <format>
                            {%Value}
                        </format>
                    </tooltip_settings>
                </bar_series>
            </data_plot_settings>
            <chart_settings>
                <title enabled="true">
                    <text>
                        徐州市图斑统计分析
                    </text>
                </title>
                <legend enabled="true" position="Bottom" align="Near">
                    <format>
                        {%Icon} {%Name} ({%Value})
                    </format>
                    <template>
                    </template>
                    <title enabled="false"/>
                    <columns_separator enabled="false"/>
                    <background>
                        <inside_margin left="10" right="10"/>
                    </background>
                </legend>
                <axes>
                    <x_axis tickmarks_placement="Center">
                        <title enabled="true">
                            <text>
                                地区
                            </text>
                        </title>
                    </x_axis>
                    <y_axis tickmarks_placement="Center">
                        <title enabled="true">
                            <text>
                                图斑数量（单位：个）
                            </text>
                        </title>
                    </y_axis>
                </axes>
            </chart_settings>
            <data>
                <series name="合法用地">
                <point name="市辖区" y="324" />
				<point name="鼓楼区" y="359" />
				<point name="云龙区" y="352" />
				<point name="九里区" y="315" />
				<point name="贾汪区" y="327" />
				<point name="泉山区" y="352" />
                <point name="丰县" y="324" />	
				<point name="沛县" y="295" />
				<point name="铜山区" y="352" />
				<point name="睢宁县" y="322" />
				<point name="新沂市" y="312" />
				<point name="邳州市" y="333" />			
                </series>
                <series name="非法用地">
                <point name="市辖区" y="245" />
				<point name="鼓楼区" y="221" />
				<point name="云龙区" y="252" />
				<point name="九里区" y="295" />
				<point name="贾汪区" y="287" />
				<point name="泉山区" y="262" />
                <point name="丰县" y="224" />	
				<point name="沛县" y="195" />
				<point name="铜山区" y="212" />
				<point name="睢宁县" y="232" />
				<point name="新沂市" y="219" />
				<point name="邳州市" y="229" />
                </series>             
            </data>
        </chart>
    </charts>
</anychart>